{
    "eslint.validate": [
        "vue",
        "html",
        "graphql",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    // "eslint.autoFixOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    }
}
